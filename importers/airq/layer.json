{
  "name": "airq",
  "title": "AirQ air quality experiment",
  "description": "Experiment about measuring air quality with No2 and PM sensors",
  "data_sources": [
    "Alphasense and Dustuino sensors, assembled by the Waag"
  ],
  "category": "environment",
  "subcategory": "airquality",
  "rdf_type": "dbpedia:Measurement",
  "licence": "CC0",
  "webservice_url": "http://services.citysdk.waag.org/airq",
  "update_rate": "60",
  "fields": [
    {
      "description": "id #",
      "name": "id",
      "type": "xsd:unsignedLong",
      "unit": "cdk:unitNone"
    },
    {
      "description": "name of the sensor",
      "name": "sensorname",
      "type": "xsd:string",
      "unit": "cdk:unitNone"
    },
    {
      "description": "The time the measurement was received",
      "name": "srv_ts",
      "type": "xsd:dateTime",
      "unit": "cdk:unitNone"
    },
    {
      "description": "The strenght of the WIFI signal",
      "name": "rssi",
      "type": "xsd:long",
      "unit": "cdk:unitNone"
    },
    {
      "description": "Temperature",
      "name": "temp",
      "type": "xsd:decimal",
      "unit": "cdk:unitDegreeCelsius"
    },
    {
      "description": "Particulate Matter size 10",
      "name": "pm10",
      "type": "xsd:decimal",
      "unit": "cdk:unitNone"
    },
    {
      "description": "Particulate Matter size 2.5",
      "name": "pm25",
      "type": "xsd:decimal",
      "unit": "cdk:unitNone"
    },
    {
      "description": "Calculated NO2 concentration",
      "name": "no2a",
      "type": "xsd:decimal",
      "unit": "cdk:unitNone"
    },
    {
      "description": "Relative Humidity percentage",
      "name": "humidity",
      "type": "xsd:decimal",
      "unit": "cdk:unitNone"
    }
  ]
}
